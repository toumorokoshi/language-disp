'scopeName': 'source.disp'
'name': 'Disp'
'fileTypes': [
  'ds'
]
'patterns': [
  {
    'match': '^\\s*#.*$'
    'name': 'comment'
  },
  {
    'match': 'mut ',
    'name': 'keyword.control.class.disp'
  }
]
